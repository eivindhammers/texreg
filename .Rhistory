is.negative <- function(v) {
v[which(!grepl("\\.", v))] <- paste0(v[which(!grepl("\\.", v))], ".")
ssp <- strsplit(v, "\\.")
negative <- logical()
for(i in length(ssp)) {
ssp <- sub("\\\\; ", "", ssp)
negative <- append(negative, grepl("^[-]", ssp))
}
any.negative <- any(negative == TRUE)
return(any.negative)
}
